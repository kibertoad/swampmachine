[
{  
   "id":"plusMinusTemplate",
   "mandatoryParameters":[  
      "statName"
   ],
   "template":{  
      "type":"swPlusMinusComposition",
      "id":"fitness",
      "labelValue":"controller.getStat('{{statName}}')",
      "labelText":"{{statName | capitalize}}",
      "plus":{  
         "text":"+",
         "onClickScript":"controller.incrementStat('{{statName}}')"
      },
      "minus":{  
         "text":"-",
         "onClickScript":"controller.decrementStat('{{statName}}')"
      }
   }
}
]